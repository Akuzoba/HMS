{
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install",
  "framework": "node",
  "startCommand": "npm start",
  "envVars": [
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "DATABASE_URL",
      "description": "Supabase PostgreSQL connection string"
    },
    {
      "key": "JWT_SECRET",
      "description": "Secret key for JWT tokens"
    }
  ]
}
